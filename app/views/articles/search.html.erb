<%= form_tag search_path, :method => :post, :id => "SearchForm" do %>
  <%= text_field_tag :search, params[:search], placeholder: 'Search your article', :autocomplete => :off, :id => 'SearchSearch'  %>
  <%= image_submit_tag( title: 'Search', class: 'search_submit', data: { "placement" => "bottom" }) %>
<% end %>